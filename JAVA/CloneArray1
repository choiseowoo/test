import java.util.*;

public class Main {
    public static void main(String[] args) throws Exception {
        int[] a = {1,2,3,4,5};
        int[] b = a.clone();
        
        b[3] = 0;
        
        for(int i =0;  i<=4; i++)
            System.out.println("c = " + b[i]);
        
            
        System.out.println("a = " + Arrays.toString(a));
        
        System.out.println("b = " + Arrays.toString(b));
        
    }
}
